package com.mauro.ejercicio01.test;

import com.mauro.utilitario.util.StringUtil;

public class PruebaTimes {
	public static void main(String[] args) {
		
		System.out.println(StringUtil.time("*", 4));
		System.out.println(StringUtil.time("java", 4));
		System.out.println(StringUtil.time("hola ", 4));
	}
}
